<section id="admin_show">
  <div class="section">
    <div class="row">
    	<div class="col s12">

		<h1>Admin</h1>


			<ul class="collapsible popout" data-collapsible="accordion">
				<li>
					<div class="collapsible-header">
						<p>Edit Homeowners</p>
					</div>       
					
					<div class="collapsible-body">
						<%= render 'home_owners/index' %>
					</div>
				
				</li>


				<li>
					<div class="collapsible-header">
						<p>Edit Adresses</p>
					</div> 
					
					<div class="collapsible-body">
						<%= render 'addresses/index' %>
					</div>
				</li>

				<li>
					<div class="collapsible-header">
						<p>Edit Issue Categories</p>
					</div> 

					<div class="collapsible-body">
						<%= render 'issue_categories/index' %>
					</div>
				</li>
			</ul>

			</div>	
		</div>
	</div>
</section>	
 <% if @issue_category_show.present? %>
     
        <div id="show_ic" class="modal">
          <div class="modal-content">            
            <%= render 'issue_categories/show', id: @issue_category_show %>            
          </div>
        </div>
        <script type="text/javascript">
          $('#show_ic').openModal();
        </script>
      <% end %>	
